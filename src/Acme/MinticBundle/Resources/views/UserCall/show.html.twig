{% extends '::base.html.twig' %}

{% block body -%}
    <h1>UserCall</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Call_id</th>
                <td>{{ entity.callid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Url_image</th>
                <td>{{ entity.urlimage }}</td>
            </tr>
            <tr>
                <th>Institution</th>
                <td>{{ entity.institution }}</td>
            </tr>
            <tr>
                <th>Academic_program</th>
                <td>{{ entity.academicprogram }}</td>
            </tr>
            <tr>
                <th>Cc</th>
                <td>{{ entity.cc }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('usercall') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('usercall_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
